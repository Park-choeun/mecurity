<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="메뉴"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/home3"
        android:layout_width="164dp"
        android:layout_height="165dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="60dp"
        android:onClick="onClick"
        app:layout_constraintEnd_toStartOf="@+id/home4"
        app:layout_constraintHorizontal_bias="0.212"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home1"
        app:srcCompat="@drawable/home3" />

    <ImageButton
        android:id="@+id/home1"
        android:layout_width="165dp"
        android:layout_height="161dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="60dp"
        android:onClick="onClick"
        app:layout_constraintEnd_toStartOf="@+id/home2"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/home1" />

    <ImageButton
        android:id="@+id/home2"
        android:layout_width="162dp"
        android:layout_height="160dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:onClick="onClick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/home2" />

    <ImageButton
        android:id="@+id/home4"
        android:layout_width="157dp"
        android:layout_height="166dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:onClick="onClick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home2"
        app:srcCompat="@drawable/home4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="실시간 CCTV"
        app:layout_constraintEnd_toEndOf="@+id/home1"
        app:layout_constraintStart_toStartOf="@+id/home1"
        app:layout_constraintTop_toBottomOf="@+id/home1" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="경보음 재생"
        app:layout_constraintEnd_toEndOf="@+id/home2"
        app:layout_constraintStart_toStartOf="@+id/home2"
        app:layout_constraintTop_toBottomOf="@+id/home2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:text="112 신고"
        app:layout_constraintEnd_toEndOf="@+id/home3"
        app:layout_constraintStart_toStartOf="@+id/home3"
        app:layout_constraintTop_toBottomOf="@+id/home3" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="주변 경찰서 찾기"
        app:layout_constraintEnd_toEndOf="@+id/home4"
        app:layout_constraintStart_toStartOf="@+id/home4"
        app:layout_constraintTop_toBottomOf="@+id/home4" />

</androidx.constraintlayout.widget.ConstraintLayout>